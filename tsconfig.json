{
  "extends": "@tsconfig/ember/tsconfig.json",
  "compilerOptions": {

    "typeRoots": ["./types", "./node_modules/@types"]
,
    "baseUrl": ".",
    "paths": {
      "a11ycat/tests/*": [
        "tests/*"
      ],
      "a11ycat/*": [
        "app/*"
      ],
      "*": [
        "types/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "types/**/*"
  ]
}
